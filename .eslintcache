 = await createUserProfile(userAuth);\n\n        userRef.onSnapshot(snapShot => {\n          setCurrentUser({\n         \n               id: snapShot.id,\n              ...snapShot.data()\n        \n          });\n\n        \n        });\n      }\n\n      setCurrentUser(userAuth );\n    });\n\n     return () => unsubscribeFromAuth = false\n }, [setCurrentUser])\n\n  return (\n      <Switch>\n        <Route \n           exact path = '/'\n           render={() =>\n              currentUser ? (\n                 <Redirect to='/chat' />\n              ) : (\n                <LandingPage />\n              )\n            }\n         />\n             <Route exact path ='/Signup' render={() =>\n              currentUser ? (\n                 <Redirect to='/chat' />\n              ) : (\n                 <SignUpPage />\n              )\n            } />\n             <Route exact path ='/Login'  render={() =>\n              currentUser ? (\n                 <Redirect to='/chat' />\n              ) : (\n                 <LoginPage/>\n              )\n            } />\n        <ProtectedRoute path='/chat' component={ChatPage} currentUser={currentUser}/>\n          \n        \n      </Switch>\n  );\n}\n\nconst mapStateToProps = state => ({\n  currentUser: state.user.currentUser\n})\n  \n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n});\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(App);\n\n","/home/ayooluwa/Documents/marvie-projects/mchatapp/src/redux/Rootreducer.js",[],"/home/ayooluwa/Documents/marvie-projects/mchatapp/src/Firebase/Firebase.js",[],"/home/ayooluwa/Documents/marvie-projects/mchatapp/src/redux/user/User.actions.js",[],"/home/ayooluwa/Documents/marvie-projects/mchatapp/src/Pages/LandingPage/LandingPage.js",["182","183","184","185","186","187","188","189","190"],"\nimport React from 'react'\nimport {Container, Row, Col} from 'react-bootstrap'\nimport PhoneIcon from '../../Assests/mob